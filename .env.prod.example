# FabricaGenerate 生产环境配置示例
# 复制此文件为 .env.prod 并填入实际值

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fabricagenerate_prod
DB_USER=fabricauser
DB_PASSWORD=your_secure_password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# JWT密钥
JWT_SECRET=your_jwt_secret_key_here

# 文件存储
UPLOAD_PATH=/var/uploads
MAX_FILE_SIZE=100MB

# API配置
API_PORT=3000
API_HOST=0.0.0.0

# 监控配置
MONITORING_ENABLED=true
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001